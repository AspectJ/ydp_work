sqlserver.url=jdbc:postgresql://192.168.195.5:5432/chuxiang_db
sqlserver.username=yidepiao
sqlserver.password=iYDP8292
#初始化连接大小
sqlserver.initialSize=1
#最小空闲数
sqlserver.minIdle=1
#最大活跃数
sqlserver.maxActive=5
#获取连接等待超时的时间
sqlserver.maxWait=60000
#配置间隔多久才进行一次检测，检测需要关闭的空闲连接，单位是毫秒
sqlserver.timeBetweenEvictionRunsMillis=60000
#配置一个连接在池中最小生存的时间，单位是毫秒
sqlserver.minEvictableIdleTimeMillis=600000
#打开PSCache，并且指定每个连接上PSCache的大小
sqlserver.poolPreparedStatements=true
#配置监控统计拦截的filters
sqlserver.filters=wall,stat