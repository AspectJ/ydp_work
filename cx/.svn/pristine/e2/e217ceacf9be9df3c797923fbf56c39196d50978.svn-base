
.yyjm{background:#fff;height:inherit}
.yyjm .leftText{width:240px;min-height:500px;float:left;background-color:#E7E8E9;padding-bottom: 50px;}
.yyjm .leftText .head{padding:15px 0px 15px 15px;background-color:#C7C8C9;color:#4C4C4C}

.option{padding:15px 0px 15px 50px;color:#666767;cursor:pointer}
.option.cur{padding:15px 0px 15px 46px;background-color:#fff;border-left:4px solid #EB002A}

.rightText{min-height:500px;float:right; background: #fff; padding-bottom: 50px;}
.rightText>div{display: none;}
#league_condition{display: block;}
.headline{background-color:#EB002A;line-height:50px;width:160px;height:50px;text-align:center;font-size:18px;color:#FFFFFF}
.content {
	padding: 0px 35px;
	padding-top: 0.5rem;
	border-top: 2px solid #EB002A;
	width: 835px;
	height: auto;
	color: dimgrey;

	line-height: 200% !important;
	color: #666767 !important;
	font-size: 16px !important;
}
.content img{width: 905px; margin-left: -2rem;}
