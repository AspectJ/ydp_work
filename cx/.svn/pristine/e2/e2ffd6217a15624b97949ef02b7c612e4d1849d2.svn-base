.yxjy{
	margin-top: 40px;
}
.yxjy .leftText{
    width: 240px;
    height: 500px;
    float: left;
    background-color: #E7E8E9;
}

.yxjy .leftText .head{
    padding: 15px 0px 15px 15px;
    background-color: #C7C8C9;
    color: #4C4C4C;
}

.option {
    padding: 15px 0px 15px 50px;
    color: #666767;
    cursor: pointer;
}

.option.cur {
	padding: 15px 0px 15px 46px;
    background-color: #fff;
    border-left: 4px solid #EB002A;
}

.rightText{
	float: right;background:#fff;
}

.headline {
	background-color: #EB002A;
	line-height: 50px;
	width: 160px;
	height: 50px;
	text-align: center;
	font-size: 18px;
	color: #FFFFFF
}

.content {
  width: 905px;
  font-size: 17px;
}

.theatre_notice, .material_load{display: none;height:auto;min-height:600px;_height:600px;}
.theatre_notice li, .material_load li{padding-bottom: 13px !important;}
.theatre_notice .content .hot{color:#eb002a}

.issue_notice{height:auto;left:0px;top:900px;width:100%;background: none !important;min-height:600px;_height:600px;}
/* .more{font-size:16px;float:right;right:20px;margin-top:-25px;color:#6E7172;cursor: pointer;} */
.search input{
	float: left;
	width: 740px;
	padding: 9px 10px;
	border: 2px solid #eb002a;
	font-size: 16px;
	outline: 0;
	font-family: Microsoft YaHei;
}
.search .search-btn{
	float: left;
	width: 136px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	font: 18px;
	color: #fff;
	background: #eb002a;
	cursor: pointer;
}

.issue_notice .content ul,.theatre_notice .content ul{padding-left:0px}
.content li{color:#666767;list-style-type:none;padding:18px 0;border-bottom:1px solid #f1f1f1;}
.content li a{color:#666767; text-decoration: none;}
.content li a:hover{color:#6495ED; text-decoration: underline;cursor: pointer;}
.content li:hover{text-decoration: none!important; color:#666767;cursor: default;}

.content .num{margin-top: -5px; margin-right:15px;background-color:#B9B8B8;color:#fff;width:28px;height:28px;line-height:28px;float:left;text-align:center;}
.content .hot{background-color:#EB002A}
.content .time{background: none !important;float:none!important;margin:-20px 0 0 790px;}

.notice{display: inline !important; float: none !important; height:225px; background: none !important;}

.material_load .content .material{ display:inline !important; float: none !important;background:none !important;}
.material_load .content .mate_title{display:inline !important; float: none !important;background: none !important;}
.material_load .content .hot{color:#EB002A}
