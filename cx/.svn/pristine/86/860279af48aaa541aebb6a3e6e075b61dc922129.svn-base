html {background: #fff; width: 100%;height: 100%; font-family: Microsoft Yahei,Hiragino Sans GB,WenQuanYi Micro Hei,sans-serif;}
body {margin: 0 auto;padding: 0;}
.body{
	width: 1190px;
	height: 90px;
	margin: 0px auto;
}
.top {
    float: right;
    height: 90px;
    line-height: 95px;
    font-size: 12px;
    color: #a5a5a5;
}
.top #userName{
	padding-right: 12px;
	border-right: 1px solid #a5a5a5;
}
.top .pointer{cursor: pointer;}
.top .out_login{cursor: pointer;margin-left: 12px;margin-right: 5px; display: none;}
.top .regist{cursor: pointer;margin-left: 12px;}

.index-wrapper{
  width: 1190px;
  margin: 0 auto 140px;
}

.title{
	width: 100%;
	height: 90px;
	line-height: 90px;
	background: #101928;
}
.title .logo{
    margin-top: 20px;
}
.title ul{
	display: inline-block;
	margin-left: 70px;
	vertical-align: top;
}
.title li{
    text-align: center;
    float: left;
    list-style-type: none;
    cursor: pointer;
    font-size: 16px;
    color: #a5a5a5;
    text-decoration: none;
    width: 70px;
    height: 87px;
    padding: 0 15px;
}
.title .title_li_cur{
	color: #fff;
    border-bottom: 3px solid #eb002a;
}
.title .city {
	margin-top: -90px;
	margin-left: 250px;
	position: absolute;
	width: 100px;
}
#cityChoice {
	padding-right: 30px;
	border: none;
	color: #666767;
	font-weight: bold;
	text-align: right;
	width: 100%;
	cursor: pointer;
	font-size: 17px;
	background-color: #F7F9FA;
	background-image: url(img/city_select.png);
	background-repeat: no-repeat;
	background-position: 110px 6px;
	background-size: 18px;
	outline:none;resize:none
}

.carousel{
    width: 100%;
    height: 445px;
		margin-bottom: 40px;
}
.carousel .Logo_img{
	width:100%;
	height:100%
}
.carousel .swiper-button-prev{
    left: 18%;
    background: url(../img/arrow.png) no-repeat;
    background-size: 200% auto;
    height: 80px;
    width: 40px;
}
.carousel .swiper-button-next{
	right: 18%;
	background: url(../img/arrow.png) no-repeat -40px;;
    background-size: 200% auto;
    height: 80px;
    width: 40px;
}
.carousel .swiper-pagination-bullet{
    width: 50px;
    height: 9px;
    display: inline-block;
    background: #fff;
    opacity: .5;
    border-radius: 0;
}
.carousel .swiper-pagination-bullet-active{
    background: #fff;
    opacity: 1;
}

.carousel .back,.carousel .next{width:40px;height:100px;position:absolute;top:140px;cursor: pointer;}
.carousel .back{left:300px}
.carousel .next{right:300px}
.carousel ul{position:absolute;top:310px;right:306px}
.carousel li{width:40px;height:5px;background-color:#fff;list-style-type:none;float:left;margin-left:9px;margin-top:3px}
.carousel .cur_li{margin-top:0px;height:7px;background-color:#EB002A}

.bottom{
	color: #666;
  font-size: 16px;
  position: absolute;
  height: 200px;
  background-color: #f2f2f2;
  left: 0px;
  width: 100%;
}
.bottom .logo{margin-left: 220px; margin-top: 40px;}
.bottom .phone{font-size: 30px;color: #eb002a;margin-left: 650px; margin-top: -60px;}
.bottom .address{margin-left: 650px; margin-top: 10px;}
.bottom .copy_right{
	font-size: 14px;
	margin: 40px auto;
	text-align: center;
}
