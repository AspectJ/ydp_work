.online_consultation{display:block; position:fixed;width:80px;height:160px;border:1px solid #B9B8B8;right:0px;bottom:200px;background-color:#F5F5F5;text-align:center;font-size:14px;color:#6E7172;line-height: 123px;}
.online_consultation .customer_service{width:100%;height:80px;border-top:1px solid #B9B8B8;background-image:url(../img/qq_over.png);background-position:27px 15px;background-repeat: no-repeat;cursor: pointer;}
.online_consultation .customer_service .font{font-weight: 600;}
.online_consultation .customer_service .online_service{
	border: 1px solid #666;
  margin-left: -146px;
  margin-top: -125px;
  background-color: #F5F5F5;
  width: 130px;
  height: 80px;
  display: none;
}
.online_consultation .customer_service .jt{position:absolute;right:79px;top:32px;}
.online_consultation .customer_service .QQ_service{height: 40px;line-height: 41px;}
.online_consultation .customer_service .QQ_service img{position: absolute;left:-133px;padding-top: 3px;}
.online_consultation .customer_service .QQ_service span{border:1px solid black;padding: 2px 4px;border-radius:10%;margin-left: 36px;cursor: pointer;}
.online_consultation .customer_service .QQ_service span:hover{background-color:#c0c0c0;}
.online_consultation .customer_service:hover{background-color: black;background-image:url(../img/qq_on.png)}

.online_consultation .back_top{width:100%;height:80px;border-top:1px solid #B9B8B8;background-repeat:no-repeat;line-height:100px;cursor:pointer; font-size: 11px;padding-top: 18px;}
.online_consultation .back_top{background-image:url(/img/online_consultation_backtop.png);background-position:27px 22px}

.mask{
	background: rgba(0, 0, 0, 0.4) none repeat scroll 0 0;
	height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    }
.mask .head{background-color:#EB002A;color:#F7F6F7;padding:14px 22px;font-weight:bold}
.mask .toolTip{
	width:450px;height:250px;background-color:#fff;
	margin:15% auto;
	border-radius: 3px;
}
.mask .substance{
	text-align: center;
	margin-top: 60px;
	color: #818181;
	font-weight: bold;
}
.mask .toolTip .sure{
	background-color: #E8E7E7;
	margin-top: 60px;
	padding: 1px 0px;
	text-align: center;
}
.mask .toolTip .sure p {
	margin: 10px auto;
	cursor: pointer;
	width: 150px;
	background-color: #EB002A;
	padding: 10px 0px;
	color: #fff;
	padding-left: 10px;
	letter-spacing: 10px;
	font-weight: bold;
	border-radius: 5px;
	box-shadow: 0 0 2px 0px #EB002A
}

.clear{clear: both;}

.content ul{;padding-left:8px;}
.content ul span{background-color:#EB002A;width:7px;height:7px;display:block;float:left;margin-top:6px;margin-right:15px}
.content li{ color:#666767;font-weight:700;font-size:15px;padding-top:5px;list-style-type: none;white-space:nowrap;text-overflow:ellipsis;overflow: hidden;}
.content li:hover{text-decoration: underline; color:#6495ED;cursor: pointer;}
.time{float: right !important; background-color: #fff !important; font-size: 14px; margin-right: 8px; color: #A7A8A8; font-weight: normal; width: auto !important; height: auto !important;}
