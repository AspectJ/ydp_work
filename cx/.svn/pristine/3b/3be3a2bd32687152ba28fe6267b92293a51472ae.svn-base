.elegant_left .theater_elegant{clear:both}

.citybox a{cursor: pointer;}

.cinemashow{min-height: 600px;background-color:#fff; width:900px;float:right; }
.cinemashow .headline{background-color:#EB002A;line-height:50px;width:160px;height:50px;text-align:center;font-size:18px;color:#FFFFFF}
.cinemashow .content{border-top:2px solid #EB002A;width:905px;height:auto;text-align:center;float:left; padding-bottom: 40px;}
.cinemashow .content img{
  width: 275px;
  height: 155px;
  margin: 20px 12px;
}
.cinemashow .content>div{margin-bottom: 20px;}
.cinemashow .content div div{
  position: absolute;
  z-index: 2;
  top: 190px;
  font-size: 15px;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  width: 100%;
  text-overflow: ellipsis;
}

.meneame{padding-bottom: 160px; }
.meneame span{display: inline-block;}

.paging{text-align:center;margin-top:40px}
.paging span{border:1px solid #CCCDCE;margin:5px;padding:2px 8px;color:#B9B8B8;cursor:pointer}
.paging .cur{color:#888989;background-color:#F3F3F3}
.paging .has{color:#888989}
.paging .none{cursor:auto}

.cx_elegant{min-height: 600px;background-color:#fff;width:900px;float:right;display:none}
.cx_elegant .headline{background-color:#EB002A;line-height:50px;width:160px;height:50px;text-align:center;font-size:18px;color:#FFFFFF}
.cx_elegant .content{border-top:2px solid #EB002A;text-align:center}
.cx_elegant .content img{padding-top:40px;width:100%}


.cx_distribution{min-height: 600px;background-color:#fff;width:900px;float:right;display:none}
.cx_distribution .headline{background-color:#EB002A;line-height:50px;width:160px;height:50px;text-align:center;font-size:18px;color:#FFFFFF}
.cx_distribution .content{border-top:2px solid #EB002A;text-align:center}
