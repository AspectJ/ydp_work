.elegant{height:80px;width:100%;border-top:2px solid #EE8295;position:absolute;top:100px;left:0px;background-color:#575757;margin:0px}
.elegant img{width:100%}
.elegant_left{min-height: 600px;width:260px;height:500px;float:left;background-color:#E7E8E9;}
.elegant_left .head{padding:15px 0px 15px 15px;background-color:#C7C8C9;color:#666767}
.elegant_left .league_theater .option,.elegant_left .theater_elegant .option{padding:15px 0px 15px 50px;color:#666767;cursor:pointer}
.elegant_left .cur{background-color:#fff;border-left:4px solid #EB002A}
