@charset "UTF-8";

.antLayoutTopaside {
  height: 100%;
  background: #ececec;
  .antLayoutWrapper {
    padding: 0 50px;
  }
  .antLayoutBreadcrumb {
    margin: 24px 0 0 45px;
  }
  .antLayoutContainer {
    height: 750px;
    position: relative;
    margin: 24px 0 0;
    padding: 24px 0;
    background: #fff;
    overflow: hidden;
  }
  .antLayoutContent {
    border-left: 1px solid #e9e9e9;
    padding: 0 24px;
    overflow: auto;
    position: relative;
    left: -1px;
  }
  .antLayoutFooter {
    height: 64px;
    line-height: 64px;
    text-align: center;
    font-size: 12px;
    color: #999;
  }
}
