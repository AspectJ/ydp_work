.left_activity{min-height: 560px;}
.left_activity .img{margin:0px auto; text-align: center;}
.left_activity .headImg{padding: 3px;width: 300px; height: 420px;}
.left_activity .view{text-align: center;line-height: 50px; color: #666;}
.left_activity .view .time{float:none !important;margin-left: 60px;font-size: 16px;color:#666767;}
.left_activity .view .see{
	margin-left: 50px;
  padding-left: 36px;
	font-size: 16px;
	color:#666;
	background: url(/img/dynamic_visit.png) no-repeat;
}
.left_activity .content h2{text-align: center;padding-bottom: 10px;}
.left_activity .content p{
	line-height: 36px;
	color: #666;
}
.left_activity .content p img{width: 740px; padding: 10px;}
.left_activity .content	.share{padding-bottom: 60px;padding-top: 10px;padding-left: 500px; color: #A7A8A8;}
.left_activity .content	.share img{vertical-align: middle; margin-left: 20px;}

.other_activity{margin-top: 120px;width: 280px;float: right;}
.other_activity .other{line-height: 50px; border: 1px solid #A7A8A8; font-size: 17px;font-weight: bold; color: #666767; padding-left: 12px;}
.other_activity ul{border: 1px solid #A7A8A8; font-size: 16px;color: #666767; padding: 15px 0 15px 20px;}
.other_activity ul li{list-style-type: none;padding: 6px 0;white-space: nowrap;width: 250px;overflow: hidden;text-overflow:ellipsis;color: #929394;}
.other_activity ul li .hot{color:#eb002a}
.other_activity ul li:hover{cursor: pointer;text-decoration: underline;color:#6495ED}

.other_activity img{border: 1px solid #A7A8A8;padding: 5px 8px; margin-top: 20px;width: 260px;height:177px;}
.other_activity div div{position:absolute; z-index:2; left:10px; top:170px;font-size:15px;font-weight:bold;color:white;overflow:hidden;white-space:nowrap;width:245px;text-overflow:ellipsis;margin:8px 2px}
.other_activity div div:hover{text-decoration: underline; color:#6495ED;cursor: pointer;}

.bottom_top{height:50px; clear:both;}
.bottom{clear: both !important;}
