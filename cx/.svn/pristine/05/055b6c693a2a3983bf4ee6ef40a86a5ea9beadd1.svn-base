.left{padding-left: 30px; margin-top: 120px;width: 860px;float: left;background-color: #fff;padding-bottom: 50px;}
#poster{width: 95%; margin: 10px auto;}
#moviename{color: #444444;}
#remark{margin-left: 80px; color: #EB002A;}
#feature{text-indent: 2rem;}
p{color: #6E6E6E; font-size: 16px;}
#stagePhoto{text-align: center; margin-top: 20px;}
#stagePhoto h2{color: #444444;text-align: left;}
#stagePhoto img{width: 80%;}
#stagePhoto .last, #stagePhoto .next{border: 1px solid #c7c8c9; width: 100px; height: 30px; line-height: 30px; display: inline-block; text-align: center; margin: 10px;cursor:pointer;}

.other{margin-top: 120px;width: 280px;float: right;}
.other div{background-color: #fff;line-height: 50px; border: 1px solid #c7c8c9; font-size: 17px;font-weight: bold; color: #666767; padding-left: 12px;}
.other ul{background-color: #fff; color: #929394; font-size: 16px; padding: 15px 0px 15px 20px; border: 1px solid #B5B5B6;}
.other li{list-style-type: none; padding: 6px 0px; width:250px;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}
.other li .noti_title:hover{text-decoration: underline; color:#6495ED;cursor: pointer;}

