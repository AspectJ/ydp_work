/*color: #e8e9ea; 边框颜色*/

.main-box .productBox{background-color:#fff;margin-top:0px;padding-top:5px;margin-left:30px;padding-left:0}
.main-box .title{width:95%;height:40px;line-height: 40px; margin: 0 auto; padding:5px 0px;font-weight:bold;color:#505052;font-size:18px}
.product{border:2px solid #e8e9ea !important;width:95%;margin-left:auto;margin-right:auto;padding-top:30px}
.productBox p{ margin-left: 20px; color: #505052; margin-bottom: 50px; }
.productBox label{ font-weight: bold; font-size: 14px; min-width: 100px; display: inline-block; text-align: left;}
.productBox p .tishi{ font-weight: bold;font-size: 14px; width: 180px; display: inline-block; text-align: center;color: red;margin-left: 8px;}
.productBox .xianshi{ font-weight: bold;font-size: 14px; width: 200px; display: inline-block; text-align: left; margin-left: 10px; margin-right: 42px;}
.productBox input{outline:medium; background-color: #fafafa;border: 1px solid #e8e9ea; margin-left: 8px; margin-right: 50px; line-height: 26px; height: 30px; width: 180px; font-size: 15px; padding-left: 10px; font-weight: bold; color: #505052;}
.productBox select{background-color: #fafafa; border: 1px solid #e8e9ea; margin-right: 50px; line-height: 30px; height: 35px; width: 195px; font-size: 15px; padding-left: 10px; font-weight: bold; color: #505052;}
.productBox textarea{ background-color: #fafafa;vertical-align: text-top; border: 1px solid #e8e9ea;  height: 100px; width: 840px; font-size: 15px; padding-left: 20px; padding-top: 10px; color: #505052;}
.productBox label .must{ color: red; font-weight: bold; margin-left: 3px;}

.productBox .image-editor{position:absolute;margin-top:-70px;margin-left:126px;width:300px;height:300px}
.productBox .image-editor .cropit-image-input, #headimg{display:none}
.productBox .image-editor .cropit-preview{border:1px solid #e8e9ea;width:200px;height:200px;margin-left:auto;margin-right:auto}
.productBox .image-editor .cropit-preview-image-container{cursor:move}
.productBox .chooseBn{width:100px;color:#fff;background-color:#383B43;padding:8px 10px;border-radius:5px;border:1px solid #e8e9ea;color:#E8E9EA;font-weight:bold;font-size:13px;margin-left:460px;text-align:center;cursor:pointer}

.cropit-image-zoom-input{margin-left:50px !important;-webkit-appearance:none;width:100%;border:none !important}
.cropit-image-zoom-input::-webkit-slider-thumb{-webkit-appearance:none;height:22px;width:12px;border-radius:3px;background-color:chocolate;cursor:pointer;margin-top:-8px;box-shadow:1px 1px 1px #000000,0px 0px 1px #0d0d0d}
.cropit-image-zoom-input::-webkit-slider-runnable-track{width:100%;height:8px;cursor:pointer;box-shadow:1px 1px 1px #000000,0px 0px 1px #0d0d0d;background:#6495ED}

.productBox .image-editor span {
	border: 1px solid;
}
.productBox .submitBn, .productBox .cancelBn,.productBox .submitBn-temp{text-align: center; margin-top: 100px; border: 2px solid #e8e9ea; width: 150px; line-height: 45px;   font-size: 20px; font-weight: bold; letter-spacing: 16px;padding-left: 20px; cursor: pointer; margin-left: 50px; margin-right: 50px;}
.productBox .submitBn{background-color: #57B382;color: #fff;}
.productBox .submitBn-temp{background-color: #57B382;color: #fff;}
.productBox .cancelBn{background-color: #fff;color: #C6C4C4;}


.passBox{ margin-top: 10px; border: 2px solid #e8e9ea; width: 95%; margin-left: auto; margin-right: auto; padding-top: 10px;}
.passBox p{ margin-left: 20px; color: #505052; margin-bottom: 50px;}
.passBox label{ font-weight: bold; font-size: 14px; width: 150px; display: inline-block; text-align: center;}
.passBox input{outline:medium; background-color: #fafafa;border: 1px solid #e8e9ea; margin-left: 10px; margin-right: 50px; line-height: 26px; height: 30px; width: 180px; font-size: 15px; padding-left: 10px; font-weight: bold; color: #505052;}
.passBox label .must{ color: red; font-weight: bold; margin-left: 3px;}

.passBox .submitBn{text-align: center; margin-top: 10px; border: 2px solid #e8e9ea; width: 150px; line-height: 45px;   font-size: 20px; font-weight: bold; letter-spacing: 16px;padding-left: 20px; cursor: pointer; margin-left: 50px; margin-right: 50px;background-color: #57B382;color: #fff;}
