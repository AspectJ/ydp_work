html,body,ul,h1,h2,h3,h4,h5,h6,p{margin:0;padding:0;font-weight:normal;}
*{font-family: "微软雅黑";}
ul{list-style: none;}
a{text-decoration: none; color:#000;}
button:focus,input:focus{outline: none;}
iframe{margin:0;padding:0;border:0; overflow: hidden;float:left;}
.clear{width:100%; height:1px; clear:both; }
.clearfix:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    height: 0;
}
.clearfix {
    height: 1%;
}
table{width:100%; border:none; border-collapse: collapse;}
s,i{font-style:normal; text-decoration: none;}
.red{color:red;}
.fl{float:left;}
.fr{float:right;}

